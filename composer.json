{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "fideloper/proxy": "~3.3",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "5.4.*",
        "predis/predis": "~1.1",
        "spatie/laravel-analytics": "~2.4",
        "spatie/laravel-backup": "~3.10",
        "watson/sitemap": "~2.0",
        "yajra/laravel-datatables-buttons": "~1.1",
        "yajra/laravel-datatables-oracle": "7.0.x-dev",
        "barryvdh/laravel-elfinder": "~0.3",
        "barryvdh/laravel-snappy": "~0.3.3",
        "baum/baum": "~1.1",
        "caffeinated/menus": "~3.0",
        "cviebrock/eloquent-sluggable": "~4.2",
        "h4cc/wkhtmltoimage-amd64": "~0.12",
        "h4cc/wkhtmltopdf-amd64": "~0.12",
        "kris/laravel-form-builder": "~1.11.0",
        "laracasts/flash": "~2.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "laracasts/generators": "dev-master",
        "laravel/browser-kit-testing": "^1.0",
        "barryvdh/laravel-debugbar": "~2.0",
        "barryvdh/laravel-ide-helper": "~2.0",
        "doctrine/dbal": "~2.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
